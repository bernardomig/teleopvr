<?xml version="1.0"?>
<launch>
    <include file="$(find openni_launch)/launch/openni.launch">
        <!-- enable depth registration -->
        <arg name="depth_registration" value="true" />
        <!-- calibration -->
        <arg name="rgb_camera_info_url" value="package://teleopvr_calibration/kinect_camera/rgb.yaml" />
        <arg name="depth_camera_info_url" value="package://teleopvr_calibration/kinect_camera/depth.yaml" />
    </include>
</launch>